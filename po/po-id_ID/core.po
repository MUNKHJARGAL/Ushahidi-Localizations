#: $lang->'config'
msgid "config file"
msgstr "File konfigurasi"

#: $lang->'controller'
msgid "controller"
msgstr "Pengendali"

#: $lang->'driver'
msgid "driver"
msgstr "Driver"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "Driver %s untuk library %s harus mengimplemen antar muka %s"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "Driver %s untuk library %s tidak ditemukan"

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Anda bisa kembali ke <a href=\"%s\">Laman muka</a> atau <a href=\"%s\">coba "
"lagi</a>"

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s<strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "Permintaan ditolak"

#: $lang->'helper'
msgid "helper"
msgstr "Pembantu"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr "Tipe file yang dikehendaki,.%s, tidak berlaku pada berkas konfigurasi"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Metode %s yang dipanggil dlm %s tidak valid"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "Properti %s pada class %s tidak ditemukan"

#: $lang->'library'
msgid "library"
msgstr "Pustaka"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "Direktori log tidak dapat digunakan untuk menulis : %s"

#: $lang->'model'
msgid "model"
msgstr "Model"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr ""
"Ushahidi tidak bisa menentukan pengendali untuk memroses permintaan ini: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "Mohon tentukan rute default di config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "Laman yang anda minta, %s, tidak ditemukan."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr "<a href=\"%s\" id=\"show_bugs\">Laporkan isu ini ke Ushahidi</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "%s, %s, yang diminta tidak ditemukan"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Stack trace"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"Dijalankan dalam {executuion_time} detik, menggunakan {memory_usage} memori. "
"Dihasilkan Ushahidi v%s"

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "Dalam satu laman permintaan Ushahidi hanya boleh ada satu masukan"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "Tidak tertangani %s pada berkas %s pada baris %s"

#: $lang->'view'
msgid "view"
msgstr "Tampilan"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr "Nama berkas untuk ditampilan harus ditentukan sebelum memanggil Render"
