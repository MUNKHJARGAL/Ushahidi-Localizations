#: $lang->'config'
msgid "config file"
msgstr "config file"

#: $lang->'controller'
msgid "controller"
msgstr "controller"

#: $lang->'driver'
msgid "driver"
msgstr "driver"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "The %s driver for the %s library must implement the %s interface"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "The %s driver for the %s library could not be found"

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Sie können auf die <a href=\"%s\">home page</a> gehen oder <a href=\"%s\"> es "
"erneut versuchen</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s <strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "Konnte die Anforderung nicht erfüllen"

#: $lang->'helper'
msgid "helper"
msgstr "helper"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr ""
"Der angeforderte Dateityp, .%s, ist nicht erlaubt entsprechend Ihren "
"Einstellungen"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Ungültige Methode %s called in %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "Die %s property existiert nicht in der %s class."

#: $lang->'library'
msgid "library"
msgstr "library"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "Das log Verzeichnis ist nicht beschreibbar: %s"

#: $lang->'model'
msgid "model"
msgstr "model"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr ""
"Ushahidi war nicht in der Lage den Controller zu bestimmen für diese "
"Anforderung: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "Bitte setzen Sie eine Standardroute in config/routes.php"

