#: 'captcha'->'default'
msgid "Please enter a valid security code."
msgstr "Zadejte, prosím, platný ověřovací kód"

#: 'captcha'->'required'
msgid "Please enter the security code."
msgstr "Vložte ověřovací kód"

#: 'email'->'email'
msgid "The Email field does not appear to contain a valid email address."
msgstr "Je emailová adresa zadána ve správném formátu?"

#: 'email'->'length'
msgid "The Email field must be at least 4 and no more 64 characters long."
msgstr "Emailová adresa musí mít ne méně než 4 a ne více než 64 znaků."

#: 'email'->'required'
msgid "The Email field is required if the checkbox is checked."
msgstr "Emailová adresa se vyžaduje, když je zatrhnutý zaškrtávací rámeček."

#: 'message'->'required'
msgid "The comments field is required."
msgstr "Políčko s komentářem je vyžadováno."

#: 'name'->'length'
msgid "The name field must be at least 3 characters long."
msgstr "Políčko se jménem musí obsahovat alespoň 3 znaky."

#: 'name'->'required'
msgid "The name field is required."
msgstr "Políčko se jménem je povinná položka."

#: 'phone'->'length'
msgid "The phone field is not valid."
msgstr "Políčko s telefonním číslem je neplatné."
