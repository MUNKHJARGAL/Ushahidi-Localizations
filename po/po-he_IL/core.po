#: $lang->'config'
msgid "config file"
msgstr "קובץ הגדרות"

#: $lang->'controller'
msgid "controller"
msgstr "בקר (controller)"

#: $lang->'driver'
msgid "driver"
msgstr "דרייבר (driver)"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "יש ליישם את הדרייבר %s עבור הספרייה %s באמצעות הממשק %s"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "לא הצלחתי למצוא את הדרייבר %s, עבור הספרייה %s"

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr "באפשרותך לגשת <a href=\"%s\">לדף הבית</a> או <a href=\"%s\">לנסות שוב</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s <strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "לא הצלחתי להשלים את הבקשה"

#: $lang->'helper'
msgid "helper"
msgstr "סייען"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr ""
"סוג הקובץ המבוקש, .%s, אינו מאושר בקובץ צפיית ההגדרות שלך (view "
"configuration file)"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "שיטה לא תקינה %s נקראה ב- %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "התכונה (property) %s איננה קיימת במחלקה (class) %s."

#: $lang->'library'
msgid "library"
msgstr "ספרייה"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "לא ניתן לכתוב לתיקיית הלוג (log): %s"

#: $lang->'model'
msgid "model"
msgstr "מודל"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr "אושהידי לא הצליח לקבוע מהו הבקר (controller) לצורך ביצוע הבקשה: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "אנא הגדר נתיב ברירת מחדל ב config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "הדף שביקשת, %s, לא נמצא."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr "<a href=\"%s\" id=\"show_bugs\">דווח על השגיאה הזו לאושהידי</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "הבקשה %s, %s, לא אותרה"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Stack Trace"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"נטען ב {execution_time} שניות, באמצעות {memory_usage} של זכרון. נוצר על ידי "
"אושהידי v%s."

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "יכול להיות רק הרצה אחת של אושהידי לכל בקשת עמוד"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "לא תפסתי %s: %s בקובץ %s בשורה %s"

#: $lang->'view'
msgid "view"
msgstr "צפייה"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr "עליך להגדיר את קובץ הצפייה (view filename) לפני שקוראים למריץ (render)"
