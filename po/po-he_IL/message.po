#: 'captcha'->'default'
msgid "Please enter a valid security code."
msgstr "נא להזין קוד אבטחה חוקי"

#: 'captcha'->'required'
msgid "Please enter the security code."
msgstr "נא להזין את קוד האבטחה"

#: 'email'->'email'
msgid "The Email field does not appear to contain a valid email address."
msgstr "נראה ששדה הדואר האלקטרוני אינו מכיל כתובת דואר אלקטרוני חוקית."

#: 'email'->'length'
msgid "The Email field must be at least 4 and no more 64 characters long."
msgstr "על הדואר האלקטרוני להיות באורך של 4 עד 64 תווים."

#: 'email'->'required'
msgid "The Email field is required if the checkbox is checked."
msgstr "שדה הדואר האלקטרוני הוא שדה נדרש אם תיבת הסימון מסומנת."

#: 'message'->'required'
msgid "The comments field is required."
msgstr "שדה ההערה הוא שדה נדרש."

#: 'name'->'length'
msgid "The name field must be at least 3 characters long."
msgstr "על השם להיות באורך 3 תווים לפחות."

#: 'name'->'required'
msgid "The name field is required."
msgstr "שדה השם הוא שדה נדרש."

#: 'phone'->'length'
msgid "The phone field is not valid."
msgstr "הטלפון אינו חוקי."
