#: $lang->'config'
msgid "config file"
msgstr "archivo de configuración"

#: $lang->'controller'
msgid "controller"
msgstr "controlador (controller)"

#: $lang->'driver'
msgid "driver"
msgstr "controlador (driver)"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "El driver %s para la biblioteca %s debe implementar la interfaz %s"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "El driver %s para la biblioteca %s no puede ser encontrado"

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Puede ir a la <a href=\"%s\">página principal</a> o <a href=\"%s\">intentar de "
"nuevo</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s <strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "No es posible completar este pedido"

#: $lang->'helper'
msgid "helper"
msgstr "ayudante (helper)"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr ""
"El tipo de archivo solicitado, .%s, no está permitido por el archivo de "
"configuración"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Método inválido %s llamado en %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "La propiedad %s no existe en la clase %s."

#: $lang->'library'
msgid "library"
msgstr "biblioteca (library)"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "El directorio de log no se puede escribir: %s "

#: $lang->'model'
msgid "model"
msgstr "modelo (model)"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr "Ushahidi no pudo encontrar un controlador para ejecutar este pedido: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "Por favor seleccione una ruta predeterminada en config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "La página solicitada, %s, no fue encontrada."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr "<a href=\"%s\" id=\"show_bugs\">Reporte esto a Ushahidi</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "El %s solicitado, %s, no fue encontrado"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Stack Trace"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"Cargado en {execution_time} segundos, utilizando {memory_usage} de memoria. "
"Generado por Ushahidi v%s."

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "Sólo puede haber una instancia de Ushahidi por cada pedido de página"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "Excepción no atrapada %: %s en el archivo %s en la línea %s"

#: $lang->'view'
msgid "view"
msgstr "vista (view)"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr "Debe seleccionar el archivo de vistas antes de llamar al render"
