#: $lang->'config'
msgid "config file"
msgstr "plik konfiguracyjny"

#: $lang->'controller'
msgid "controller"
msgstr "kontroler"

#: $lang->'driver'
msgid "driver"
msgstr "sterownik"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr ""
"Sterownik %s sterownik dla biblioteki %s musi wykorzystywać interface %s"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "Sterownik %s dla biblioteki %s nie został znaleziony"

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Możesz przejść do <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>% <strong>[%]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "Żądanie nie może być zrealizowane"

#: $lang->'helper'
msgid "helper"
msgstr "pomocnik"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr ""
"Żądany typ pliku, .%s, nie jest dozwolony w twoim pliku konfiguracji  widoku"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Nieprawidłowa metoda %s zastosowana w %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "Właściwość %s nie istnieje w klasie %s."

#: $lang->'library'
msgid "library"
msgstr "biblioteka"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "Ten katalog logowania nie jest zapisywalny: %s"

#: $lang->'model'
msgid "model"
msgstr "model"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr ""
"Ushahidi nie był w stanie określić kontrolera do przetworzenia tego żądania: "
"%s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "Określ domyślną ścieżkę w config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "Żądana strona, %s, nie została znaleziona."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr "<a href=\"%s\">prześlij ten problem do Ushahidi</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "Żądane %s, %s, nie mogą być znalezione"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Stack Trace"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"Przesłane w {czas wykonania} sekund, przy użyciu {używana pamięć} pamięci. "
"Wygenerowane przez Ushahidi v%s."

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "Może być tylko jeden przypadek Ushahidi na żądanie strony"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "Niewychwycone %s: %s w pliku %s na linii %s"

#: $lang->'view'
msgid "view"
msgstr "widok"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr "Musisz określić nazwę pliku widoku przed obrazowaniem"
