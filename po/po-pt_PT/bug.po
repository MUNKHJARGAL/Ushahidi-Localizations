#: 'captcha'->'default'
msgid "Please enter a valid security code."
msgstr "Por favor insira um código de segurança válido"

#: 'captcha'->'required'
msgid "Please enter the security code."
msgstr "Por favor insira o código de segurança"

#: 'email'->'email'
msgid "The Email field does not appear to contain a valid email address?"
msgstr "O campo do Email parece conter um endereço de email inválido?"

#: 'email'->'length'
msgid "The Email field must be at least 4 and no more 64 characters long."
msgstr "O campo do Email deve ter entre 4 a 64 caracteres."

#: 'email'->'required'
msgid "The Email field is required if the checkbox is checked."
msgstr "O campo do Email é obrigatório caso a opção seja seleccionada."

#: 'error'->'required'
msgid "The error field is required."
msgstr "O campo do erro é obrigatório."

#: 'subject'->'length'
msgid "The subject field must be at least 3 characters long."
msgstr "O campo do assunto deve ter pelo menos 3 caracteres."

#: 'subject'->'required'
msgid "The subject field is required."
msgstr "O campo do assunto é obrigatório."

#: 'yourname'->'length'
msgid "The name field must be at least 3 characters long."
msgstr "O campo do nome deve ter pelos 3 caracteres."

#: 'yourname'->'required'
msgid "The name field is required."
msgstr "O campo do nome é obrigatório."
