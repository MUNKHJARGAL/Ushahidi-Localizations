#: $lang->'config'
msgid "config file"
msgstr "конфігураційний файл"

#: $lang->'controller'
msgid "controller"
msgstr "контролер"

#: $lang->'driver'
msgid "driver"
msgstr "драйвер"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "%s драйвер для бібліотеки %s мусить надавати інтерфейс %s"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "%s драйвер для %s бібліотеки не може бути знайдено"

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Ви можете перейти на сторінку <a href=\"%s\">головна</a> або <a href=\"%"
"s\">спробувати ще раз</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s <strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "Неможливо виконати запит"

#: $lang->'helper'
msgid "helper"
msgstr "помічник"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr "Шуканий тип файлу, .%s, не допускається у конфігураційному файлі"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Невірний метод %s викликається у %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "Властивість %s не існує у класі %s"

#: $lang->'library'
msgid "library"
msgstr "бібліотека"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "Каталог для лог-файлів не доступний для запису: %s"

#: $lang->'model'
msgid "model"
msgstr "модель"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr ""
"Ushahidi була не в змозі визначити контролер для обробки цього запиту: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "Будь ласка, встановіть маршрут за замовчанням у config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "Сторінка, яку ви просили, %s, не може бути знайдена."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr " <a href=\"%s\" id=\"show_bugs\">Розказати про проблему Ushahidi</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "<a href=\"%s\" id=\"show_bugs\">Повідомте Ushahidi про цю проблему</a>"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Трасування стеку"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"Завантажено протягом {execution_time} секунд, використовуючи {memory_usage} "
"памяті. Згенеровано Ushahidi v%s."

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "Можлива лише один екземпляр Ushahidi на сторінку запиту"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "Неперехоплене %s: %s у файлі %s рядок %s"

#: $lang->'view'
msgid "view"
msgstr "вид"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr "Ви повинні встановити вид файлу перед викликом рендера"
