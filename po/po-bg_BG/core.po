#: $lang->'config'
msgid "config file"
msgstr "конфигурационен файл"

#: $lang->'controller'
msgid "controller"
msgstr "контролер"

#: $lang->'driver'
msgid "driver"
msgstr "драйвер"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "Драйверът %s за библиотеката %s трябва да имплементира интерфейса %s"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "Драйверът %s за библиотеката %s не беше намерен"

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Може да отидете на <a href=\"%s\">началната страница</a> или да <a href=\"%"
"s\">опитате отново</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s <strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "Заявката не беше успешно извършена"

#: $lang->'helper'
msgid "helper"
msgstr "помощник"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr "Заявеният тип файл, .%s, не е сред позволените за преглед"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Неправилен метод %s беше извикан в %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "Променливата %s не съществува в клас %s."

#: $lang->'library'
msgid "library"
msgstr "библиотека"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "Невъзможно е писането в папката за log файлове: %s"

#: $lang->'model'
msgid "model"
msgstr "модел"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr "Ushahidi не можа да открие контролер за обработка на тази заявка: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "Моля задайте път по подразбиране в  config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "Страницата, която поискахте, %s, не беше открита."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr "<a href=\"%s\" id=\"show_bugs\">Съобщете за тази грешка на Ushahidi</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "Искания %s, %s, не беше открит"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Път на изпълнение"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"Зареден за {execution_time} секунди, използвайки {memory_usage} от паметта. "
"Създадено от Ushahidi v%s."

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "Може да има само един Ushahidi на заявка за страница"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "Нехванато %s: %s във файл %s на ред %s"

#: $lang->'view'
msgid "view"
msgstr "преглед"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr "Първо трябва да зададете файла за преглед преди да извикате изписването"
