#: $lang->'config'
msgid "config file"
msgstr "arquivo config"

#: $lang->'controller'
msgid "controller"
msgstr "controlador"

#: $lang->'driver'
msgid "driver"
msgstr "driver"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "O driver %s para a coleção %s deve ser implementado na interface do %s"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "Não foi possível encontrar o driver %s para a coleção %s."

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Acesse a <a href=\"%s\">página inicial</a> ou <a href=\"%s\">tente "
"novamente</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s <strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "Não foi possível completar a solicitação"

#: $lang->'helper'
msgid "helper"
msgstr "assistente"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr ""
"A solicitação desse tipo de arquivo, .%s, não é permitido durante "
"visualização da configuração do arquivo"

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Método inválido %s solicitado em %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "A propriedade %s não existe na classe %s."

#: $lang->'library'
msgid "library"
msgstr "coleção"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "O diretório não pode ser escrito: %s"

#: $lang->'model'
msgid "model"
msgstr "modelo"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr ""
"Ushahidi não conseguiu determinar um controlador para processar a "
"solicitação: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr "Defina um route padrão em config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "Não foi possível encontrar a página solicitada: %s."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr "<a href=\"%s\">Relate esse problema ao Ushahidi</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "A solicitação %s, %s, não pode ser encontrada"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Stack Trace"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"Carregado em {execution_time} segundos, usando {memory_usage} de memória. "
"Gerado pelo Ushahidi v%s."

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "Só é possível haver uma versão do Ushahidi por página solicitada"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "%s não captado: %s no arquivo %s linha %s"

#: $lang->'view'
msgid "view"
msgstr "visualizar"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr ""
"É preciso determinar visualização do arquivo antes de solicitar renderização"
