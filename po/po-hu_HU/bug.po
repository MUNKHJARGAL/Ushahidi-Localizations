#: 'captcha'->'default'
msgid "Please enter a valid security code."
msgstr "Kérem, adjon meg érvényes biztonsági kódot"

#: 'captcha'->'required'
msgid "Please enter the security code."
msgstr "Kérem, adja meg a biztonsági kódot"

#: 'email'->'email'
msgid "The Email field does not appear to contain a valid email address?"
msgstr "Érvénytelen e-mail címet adott meg."

#: 'email'->'length'
msgid "The Email field must be at least 4 and no more 64 characters long."
msgstr "Az e-mail mezőben legalább 4 és nem több mint 64 karakter szerepelhet."

#: 'email'->'required'
msgid "The Email field is required if the checkbox is checked."
msgstr "Az e-mail megadása kötelező, ha a jelölőnégyzet ki van pipálva."

#: 'error'->'required'
msgid "The error field is required."
msgstr "A hiba mező kitöltése kötelező."

#: 'subject'->'length'
msgid "The subject field must be at least 3 characters long."
msgstr "A témának legalább 3 karakter hosszúnak kell lennie."

#: 'subject'->'required'
msgid "The subject field is required."
msgstr "A téma mező kitöltése kötelező."

#: 'yourname'->'length'
msgid "The name field must be at least 3 characters long."
msgstr "A névnek legalább 3 karakter hosszúnak kell lennie."

#: 'yourname'->'required'
msgid "The name field is required."
msgstr "A név mező kitöltése kötelező."
