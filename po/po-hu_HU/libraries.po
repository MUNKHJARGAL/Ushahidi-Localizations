#: $lang->'akismet_cannot_connect'
msgid "Could not connect to Akismet server."
msgstr "Nem tudtam kapcsolódni az Akismet spamszűrő szerveréhez."

#: $lang->'akismet_cannot_retrieve'
msgid "The response from Akismet could not be retrieved."
msgstr "Az Akismet spamszűrő válasza nem elérhető."

#: $lang->'api_library_not_found'
msgid ""
"The API library: <strong>%s</strong> for the <strong>%s</strong> class could "
"not be found. Please check your API task routing table."
msgstr ""
"Az API könyvtár: <strong>%s</strong>, mint a <strong>%s</strong> osztálya "
"nem található. Ellenőrizze az API utasítás routing táblázatát."

#: $lang->'askimet_invalid_apikey'
msgid "Your Akismet API key is not valid."
msgstr "Az Akismet API kulcsa érvénytelen."

#: $lang->'clickatell_fopen_error'
msgid ""
"Error while executing fopen sending method!<br>Please check whether PHP has "
"OpenSSL support and whether the PHP version is greater than 5.2."
msgstr ""
"Hiba történt a fopen küldési mód használata közben. Ellenőrizze, hogy a PHP-"
"nak van-e OpenSSL támogatottsága, és hogy a PHP verzió jobb-e az 5.2-nél."

#: $lang->'clickatell_message_too_long'
msgctxt "$lang->'clickatell_message_too_long'"
msgid "Your unicode message is too long! (Current length="
msgstr "Unicode üzenete túl hosszú! (Jelenlegi hossz="

#: $lang->'clickatell_no_destination_address'
msgid "Please specify a destination address (TO)!"
msgstr "Kérem, adjon meg egy rendeltetési címet (TO)!"

#: $lang->'clickatell_no_sender_address'
msgid "Please specify source address (FROM)!"
msgstr "Kérem, adja meg a forráscímet (FROM)!"

#: $lang->'clickatell_unicode_message_too_long'
msgctxt "$lang->'clickatell_unicode_message_too_long'"
msgid "Your unicode message is too long! (Current length="
msgstr "Unicode üzenete túl hosszú! (Jelenlegi hossz="

#: $lang->'clickatell_unsupported_method'
msgid "Unsupported sending method!"
msgstr "A küldési mód nem támogatott!"

#: $lang->'invalid_api_library'
msgid ""
"The API library %s is invalid. All API libraries must be subclasses of %s."
msgstr ""
"Az API könyvtár %s érvénytelen. Minden API könyvtárnak egy %s alosztálynak "
"kell lennie."

#: $lang->'upgrade_directory_not_deleted'
msgid "Directory <code>%s</coded> could not be deleted."
msgstr "A <kód>%s</coded> könyvtár nem törölhető."

#: $lang->'upgrade_extracting_error'
msgid "Error while extracting: <code>%s</code>"
msgstr "Hiba történt a következő kibontása közben: <code>%s</code>"

#: $lang->'upgrade_failed'
msgid "Downloading the latest Ushahidi failed. HTTP status code"
msgstr "A legutóbbi ushahidi fájl letöltése sikertelen. HTTP státuszkód"

#: $lang->'upgrade_file_not_copied'
msgid "File <code>%s</coded> could not be copied."
msgstr "A <code>%s</coded> fájl nem másolható."

#: $lang->'upgrade_file_not_deleted'
msgid "File <code>%s</coded> could not be deleted."
msgstr "A <code>%s</coded> fájl nem törölhető."

#: $lang->'upgrade_title'
msgid "Ushahidi upgrade script"
msgstr "Ushahidi frissítés leírása"

#: $lang->'upgrade_zip_error'
msgid "The downloaded Ushahidi zip file <code>%s</code>, could not be written."
msgstr "A <code>%s</coded> letöltött ushahidi zip fájl nem írható."

