#: 'captcha'->'default'
msgid "Please enter a valid security code."
msgstr "Kérem, adjon meg érvényes biztonsági kódot."

#: 'captcha'->'required'
msgid "Please enter the security code."
msgstr "Kérem, adja meg a biztonsági kódot."

#: 'email'->'email'
msgid "The Email field does not appear to contain a valid email address."
msgstr "Az email-mező nem tartalmaz érvényes email-címet."

#: 'email'->'length'
msgid "The Email field must be at least 4 and no more 64 characters long."
msgstr "Az email-mező legalább 4 legfeljebb 64 karakter hosszúságú lehet."

#: 'email'->'required'
msgid "The Email field is required if the checkbox is checked."
msgstr "Az emailmező kitöltése kötelező, ha a jelölőnégyzetet bejelölte."

#: 'message'->'required'
msgid "The comments field is required."
msgstr "A megjegyzés mező kitöltése szükséges."

#: 'name'->'length'
msgid "The name field must be at least 3 characters long."
msgstr "A névmezőnek legalább 3 karakter hosszúnak kell lennie."

#: 'name'->'required'
msgid "The name field is required."
msgstr "A névmező kitöltése kötelező."

#: 'phone'->'length'
msgid "The phone field is not valid."
msgstr "A telefonszám mező érvénytelen."
