#: $lang->'config'
msgid "config file"
msgstr "konfiguracios fajl"

#: $lang->'controller'
msgid "controller"
msgstr "vezérlő"

#: $lang->'driver'
msgid "driver"
msgstr "meghajtó"

#: $lang->'driver_implements'
msgid "The %s driver for the %s library must implement the %s interface."
msgstr "A %s meghajtónak a %s könyvtárhoz a %s interfészt kell implementálnia"

#: $lang->'driver_not_found'
msgid "The %s driver for the %s library could not be found."
msgstr "A %s meghajtó a %s könyvtárhoz nem található."

#: $lang->'errors_disabled'
msgid ""
"You can go to the <a href=\"%s\">home page</a> or <a href=\"%s\">try again</a>."
msgstr ""
"Látogasson el a <a href=\"%s\">főoldalra</a> vagy <a href=\"%s\">próbálja "
"újra</a>."

#: $lang->'error_file_line'
msgid "<tt>%s <strong>[%s]:</strong></tt>"
msgstr "<tt>%s <strong>[%s]:</strong></tt>"

#: $lang->'generic_error'
msgid "Unable to Complete Request"
msgstr "A kérést nem sikerült teljesíteni."

#: $lang->'helper'
msgid "helper"
msgstr "helper"

#: $lang->'invalid_filetype'
msgid ""
"The requested filetype, .%s, is not allowed in your view configuration file."
msgstr ""
"A kért fájltípust, .%s, nem támogatja a nézethez tartozó konfigurációs fájl."

#: $lang->'invalid_method'
msgid "Invalid method %s called in %s"
msgstr "Érvénytelen folyamatot %s indított el a %s"

#: $lang->'invalid_property'
msgid "The %s property does not exist in the %s class."
msgstr "A %s property fájl nem található a %s osztályban."

#: $lang->'library'
msgid "library"
msgstr "könyvtár"

#: $lang->'log_dir_unwritable'
msgid "The log directory is not writable: %s"
msgstr "A log könyvtár nem írható: %s'"

#: $lang->'model'
msgid "model"
msgstr "modell"

#: $lang->'no_controller'
msgid ""
"Ushahidi was not able to determine a controller to process this request: %s"
msgstr ""
"Az 'Ushahidi nem tudott controllert meghatározni a kérés feldolgozásához: %s"

#: $lang->'no_default_route'
msgid "Please set a default route in config/routes.php"
msgstr ""
"Kérem, adjon meg egy alapértelmezett elérési utat a következőn: "
"config/routes.php"

#: $lang->'page_not_found'
msgid "The page you requested, %s, could not be found."
msgstr "A kért oldal, %s, nem található."

#: $lang->'report_bug'
msgid "<a href=\"%s\" id=\"show_bugs\">Report This Issue To Ushahidi</a>"
msgstr "<a href=\"%s\" id=\"show_bugs\">Jelentem ezt az esetet az Ushahidinek</a>"

#: $lang->'resource_not_found'
msgid "The requested %s, %s, could not be found."
msgstr "A kért %s, %s, nem található"

#: $lang->'stack_trace'
msgid "Stack Trace"
msgstr "Stack Trace"

#: $lang->'stats_footer'
msgid ""
"Loaded in {execution_time} seconds, using {memory_usage} of memory. "
"Generated by Ushahidi v%s."
msgstr ""
"A betöltéshez igénybevett idő {execution_time} másodperc, felhasznált "
"memória {memory_usage}. Az Ushahidi v%s által létrehozva."

#: $lang->'there_can_be_only_one'
msgid "There can be only one instance of Ushahidi per page request."
msgstr "Csak egy Ushahidi eset lehet egy oldalkérésen"

#: $lang->'uncaught_exception'
msgid "Uncaught %s: %s in file %s on line %s"
msgstr "Nem talált %s: %s %s fájlban %s sorban"

#: $lang->'view'
msgid "view"
msgstr "nézet"

#: $lang->'view_set_filename'
msgid "You must set the the view filename before calling render."
msgstr "Beküldés előtt a nézet fájlnevet kell meg kell adni"
